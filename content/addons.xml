<?xml version='1.0' encoding='utf-8'?>
<addons>
  <addon id="plugin.video.nghiencodew" name="Nghiện Code" provider-name="HoangTan" version="1.0.5">
    <extension point="xbmc.python.pluginsource" library="addon.py">
      <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary lang="en_GB">Nghiện Code.</summary>
      <description lang="en_GB">Nothing to say</description>
      <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
      <platform>all</platform>
      <language>en</language>
      <assets>
        <icon>icon.png</icon>
        <fanart>fanart.jpg</fanart>
      </assets>
      <reuselanguageinvoker>true</reuselanguageinvoker>
      <supportedcontent>
        <content>tvshows</content>
        <content>movies</content>
      </supportedcontent>
    </extension>
  </addon>

  <addon id="script.module.axel.downloader" name="Axel Downloader" version="0.0.1" provider-name="Eldorado">
    <requires>
      <import addon="xbmc.python" version="2.1.0" />
    </requires>
    <extension point="xbmc.python.module" library="lib" />
    <extension library="proxy.py" point="xbmc.service" start="startup" />
    <extension point="xbmc.addon.metadata">
      <platform>all</platform>
      <summary lang="en">Multi-connection/threaded Downloader</summary>
      <description lang="en">Download a file using multiple connections to achieve the highest speed.</description>
    </extension>
  </addon>


</addons>